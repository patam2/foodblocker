{
    "name": "Toidublokeerija",
    "description": "Base Level Extension",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
      "default_popup": "/settings/index.html",
      "default_icon": "icon"
    },
    "content_scripts": [
      {
        "matches": ["*://*.selver.ee/*"],
        "js": ["jquery-3.6.0.js", "worker.js"],
        "css": ["filter.css"]
      }
    ],
    "background": {
      "service_worker": "background.js"
    },
    "permissions": [
      "tabs", "storage"
    ]
}